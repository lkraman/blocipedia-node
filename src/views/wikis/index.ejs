<!DOCTYPE html>
<html>
  <head>

    <% include ../static/partials/head.ejs %>

  </head>
  <body>
    <main class="container">

      <% include ../static/partials/navbar.ejs %>

      <h1>Wikis</h1>
   
      <ul class="list-group">
        <% wikis.forEach((wiki) => { %>
          <li class="list-group-item">
           <a href="/wikis/<%= wiki.id %>"> <%= wiki.title %> </a>         
          </li>
          <a href="/wikis/<%= wiki.id %>/wikis/new" class="btn btn-success">New Wiki</a>         
        <% }) %>
      </ul>
      
    </main>
  </body>
</html>